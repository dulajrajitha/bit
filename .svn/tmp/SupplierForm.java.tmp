private void saveActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_saveActionPerformed
        
    try{
        SupplierDto bean = new SupplierDto();
        bean.setName(name.getText());
        bean.setOtherName(otherName.getText());
        bean.setPobox(pobox.getText());
        bean.setStreet(street.getText());
        bean.setCity(city.getText());
        bean.setTel(tel.getText());
        bean.setMobile(mobile.getText());
        bean.setFax(fax.getText());
        bean.setEmail(email.getText());
        
        SaveSupplierController controller = new SaveSupplierController(bean);
        controller.execute();
    }catch(Exception e){

    }        
        
        
        
}//GEN-LAST:event_saveActionPerformed

